<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>escribirFecha</servlet-name>
    <servlet-class>EscribeFecha</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>inventarNumero</servlet-name>
    <servlet-class>InventarNumero</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>escribirFecha</servlet-name>
    <url-pattern>/escribeFecha</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>inventarNumero</servlet-name>
    <url-pattern>/inventarNumero</url-pattern>
  </servlet-mapping>
  
</web-app>